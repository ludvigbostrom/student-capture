@RequestMapping(value = "/assignment"


@RequestMapping(value = "/DB")
    @RequestMapping(value = "getGrade", method = RequestMethod.GET)
    @RequestMapping(value = "/createAssignment", method = RequestMethod.POST)
    @RequestMapping(value = "/createAssignmentVideo/{courseCode}/{courseID}/{assignmentID}", method = RequestMethod.POST)
    @RequestMapping(value = "/setGrade", method = RequestMethod.POST)
    @RequestMapping(value = "/setFeedback", method = RequestMethod.POST)
    @RequestMapping(value = "/getAssignmentVideo/{courseCode}/{courseId}/{assignmentId}",
    @RequestMapping(value = "/getFeedbackVideo", method = RequestMethod.GET, produces = "video/webm")
    @RequestMapping(value = "/getAssignmentInfo", method = RequestMethod.POST)
    @RequestMapping(value = "/userNameExist", method = RequestMethod.GET)
    @RequestMapping(value = "/userEmailExist", method = RequestMethod.GET)
    @RequestMapping(value = "/addUser" RequestMethod.GET)
    @RequestMapping(value = "/getHpswd", method = RequestMethod.GET)
    @RequestMapping(value = "/addCourse" method = RequestMethod.POST)
    @RequestMapping(value = "/addCourseWithTeacher", method = RequestMethod.POST)
    @RequestMapping(value = "/getCourse", method = RequestMethod.GET)
    @RequestMapping(value = "/addParticipant", method = RequestMethod.POST)
    @RequestMapping(value = "/getFunctionForParticipant", method = RequestMethod.GET)
    @RequestMapping(value = "/getAllCoursesIDsForParticipant",  method = RequestMethod.GET)
    @RequestMapping(value = "/getAllParticipantsFromCourse", method = RequestMethod.GET)
    @RequestMapping(value = "/getAllSubmissions", method = RequestMethod.GET)
    @RequestMapping(value = "/getAllUngradedSubmissions", method = RequestMethod.GET)
    @RequestMapping(value = "/getAllSubmissionsWithStudents", method = RequestMethod.GET,)
    @RequestMapping(value = "/getHierarchy", method = RequestMethod.GET)
    @RequestMapping(value = "/addSubmission/{courseCode}/{courseID}/{assignmentID}/{userID}", method = RequestMethod.POST)

@RequestMapping(value = "feedback")
    @RequestMapping(value = "get", method = RequestMethod.GET)
    @RequestMapping(value = "video", method = RequestMethod.GET)
    @RequestMapping(value = "set", method = RequestMethod.POST)

@RequestMapping(value="/video/download")
@RequestMapping(value="/video/inrequest", method = RequestMethod.GET)
@RequestMapping(value="/video/outrequest", method = RequestMethod.GET)
